  "info": {
    "description": "\n    A REST API service for user authentication.\n\n    This REST API:\n    - Allows users to register and login.\n    - Generates access tokens and refresh tokens.\n    - Allows users to refresh their access tokens.\n    - Allows users to logout.\n    - Allows users to view and revoke their refresh tokens.\n    ",
    "title": "Microservices [auth]",
    "version": "latest"
  },
  "openapi": "3.1.0",
  "paths": {
    "/login": {
      "post": {
        "description": "Login a user and return an access token.\n\nArgs:\n    response (Response): The response object.\n    request (Request): The request object.\n    ip_address (str): The client IP address.\n    session (AsyncSession): The database session.\n    form_data (OAuth2PasswordRequestForm): The form data.\n\nReturns:\n    Token: The access token.",
        "operationId": "login_login_post",
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/Body_login_login_post"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Token"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Login",
        "tags": [
          "auth"
        ]
      }
    },
    "/logout": {
      "post": {
        "description": "Logout current user.\n\nArgs:\n    session (AsyncSession): The database session.\n    consumed_refresh_token (RefreshToken): The consumed refresh token.\n\nReturns:\n    dict: The logout message.",
